{
  "name": "tap_complex",
  "version": "0.1.0",
  "scripts": {
    "prebuild" : "babel api -d /tmp/$npm_package_name/build;mkdir -p build",
    "build": "./node_modules/.bin/uglifyjs /tmp/$npm_package_name/build/**/*.js -c passes=2 -o build/$npm_package_name-$npm_package_version.min.js",
    "postbuild" : "rm -rf /tmp/$npm_package_name"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "uglify-js": "^3.14.2"
  }
}
